<Window x:Class="BlueSky.Commands.File.SelectTableWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Select Worksheet Window" Height="243" Width="241" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid Height="200" Width="219">
        <ListBox Height="115" HorizontalAlignment="Left" Margin="21,38,0,0" Name="tablelist" VerticalAlignment="Top" Width="175" />
        <Label Content="Select One Table/Sheet" Height="28" Margin="21,9,23,0" Name="label1" VerticalAlignment="Top" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="21,159,0,0" Name="okbutton" VerticalAlignment="Top" Width="49" Click="button1_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="147,159,0,0" Name="CancelButton" VerticalAlignment="Top" Width="49" Click="CancelButton_Click" />
    </Grid>
</Window>
